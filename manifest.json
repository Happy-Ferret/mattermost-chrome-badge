{
  "name": "CHANGE THIS : Extension boilerplate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://192.168.47.60:8065/pharmaco",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "96": "icons/icon96.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.js","src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs"
  ]
}
