{
  "name": "mattermost-chrome-badge",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://192.168.47.60:8065/pharmaco",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "96": "icons/icon96.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.js","src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon96.png",
    "default_title": "Mattermost Notifications"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
